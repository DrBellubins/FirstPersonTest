	vec3 n_out12p0 = mix(n_in12p0, vec3(n_out20p0.xyz), vec3(n_out13p0));


// Output:0
	ALBEDO = vec3(n_out7p0.xyz);
	NORMAL_MAP = n_out12p0;


}

void light() {
